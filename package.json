{
  "name": "bullish-beginner-scanner",
  "private": true,
  "version": "0.1.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "dev:app": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 concurrently \"npm:dev:web\" \"npm:dev:electron\"",
    "dev:web": "vite",
    "dev:electron": "wait-on tcp:5173 && electron electron/main.cjs",
    "build:app": "npm run build && electron-builder -c.extraMetadata.main=electron/main.cjs",
    "release": "npm run build && electron-builder --publish always -c.extraMetadata.main=electron/main.cjs",
    "build:win": "npm run build && electron-builder --win -c.extraMetadata.main=electron/main.cjs",
    "build:win:unsigned": "set CSC_IDENTITY_AUTO_DISCOVERY=false&& npm run build && electron-builder --win -c.extraMetadata.main=electron/main.cjs",
    "cut:patch": "npm version patch -m \"chore: release %s\" && git push && git push --tags",
    "cut:minor": "npm version minor -m \"chore: release %s\" && git push && git push --tags",
    "cut:major": "npm version major -m \"chore: release %s\" && git push && git push --tags"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.7",
    "@radix-ui/react-popover": "^1.0.7",
    "lucide-react": "^0.441.0",
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "electron": "^31.4.0",
    "electron-builder": "^24.13.3",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "cross-env": "^7.0.3"
  }
}
